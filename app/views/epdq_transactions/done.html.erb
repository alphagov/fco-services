<main id="content" role="main" class="group">

  <header class="page-header group">
    <div>
      <h1><span>Service</span> <%= @transaction.title %></h1>
    </div>
  </header>
  <div class="article-container group">
    <article role="article" class="group">
      <div class="inner">
        <%= render "partials/transactions/#{@transaction.slug}/done" %>

        <section class="done">
          <p>
            <a href="https://www.gov.uk/done/<%= @transaction.slug %>" class="button" rel="external">Finished</a>
          </p>
        </section>
      </div>
    </article>
  </div>

</main>
